{
    "manifest_version": 3,
    "name": "Youtube Ad Skipper",
    "description": "This is an extension that automatically skips ads that have a skip ads button.",
    "version": "1.0.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
        }
    ]
}